<?xml version='1.0' encoding='utf-8'?>
<widget id="com.gse_cordova_plugins.example" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>GseCordovaPluginsExample</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="ios" spec="~4.5.5" />
    <plugin name="com.gse.gespay" spec="/Users/fjl/GseCordovaPlugins/cordova-plugin-gsepay" />
    <plugin name="cordova-plugin-gsepay" spec="../GsePay" />
    <plugin name="cordova-plugin-gsewallet" spec="/Users/fjl/Projects/质享科技项目/ges-wallet-ios-cordova/GseCordovaPlugins/GseWallet" />
</widget>
